#include <iostream>

using namespace std;

int calcularFactorial(int n)
{
   int fact=1;
   for(int i=1;i<=n;i++)
   {
       fact*=i;
   }
   return fact;
}

int main ()
{
    int n;
    cout <<"Ingresar el nÃºmero que quiere calcular: ";
    cin>>n;
    
    cout<<"El factorial de "<<n<<" es:"<<calcularFactorial(n);
    
    return 0;
}
